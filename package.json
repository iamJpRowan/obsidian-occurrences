{
  "name": "obsidian-occurrences",
  "version": "1.0.0",
  "description": "A plugin for managing and viewing occurrences (time-stamped events) in your Obsidian vault.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add src/manifest.json manifest.json versions.json",
    "prepare": "husky",
    "lint": "eslint src/**/*.ts"
  },
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "occurrences",
    "events",
    "time-stamped",
    "notes",
    "vault",
    "markdown"
  ],
  "author": "Jp Rowan",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@eslint/json": "^0.14.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "eslint": "^9.39.1",
    "eslint-plugin-obsidianmd": "^0.1.9",
    "glob": "^11.0.1",
    "husky": "^9.1.7",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.48.0"
  }
}
